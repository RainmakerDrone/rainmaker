<h1>Listing votes</h1>

<table>
  <tr>
    <th>Time</th>
    <th>Phone Number</th>
    <th>Vote</th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <td><%= time_ago_in_words(vote.created_at) %></td>
    <td><%= vote.name %></td>
    <td><%= vote.description.upcase %></td>
  </tr>
<% end %>
</table>

<br />